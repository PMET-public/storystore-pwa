{
  "name": "luma-pwa",
  "version": "0.0.1--alpha",
  "main": "server.js",
  "author": "Carlos A. Cabrera <cacabrer@adobe.com>",
  "license": "MIT",
  "scripts": {
    "predev": "rimraf dist",
    "dev": "yarn server:build && node dist/server.js",
    "prebuild": "rimraf dist",
    "build": "yarn server:build && yarn next:build",
    "next:build": "next build",
    "server:build": "tsc --outDir dist --esModuleInterop server.ts",
    "server:watch": "tsc --outDir dist --esModuleInterop --watch --preserveWatchOutput server.ts",
    "start": "NODE_ENV=production node dist/server.js"
  },
  "dependencies": {
    "@apollo/react-hooks": "^0.1.0-beta.11",
    "apollo-boost": "^0.4.3",
    "apollo-client": "^2.6.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "graphql": "^14.4.2",
    "isomorphic-unfetch": "^3.0.0",
    "luma-storybook": "https://github.com/PMET-public/luma-storybook#master",
    "next": "^9.0.3",
    "next-offline": "^4.0.2",
    "preact": "^8.4.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.8",
    "@types/request": "^2.48.2",
    "rimraf": "^2.6.3",
    "webpack": "^4.38.0",
    "webpack-pwa-manifest": "^4.0.0"
  }
}
