{
    "defaultSeverity": "error",
    "extends": ["tslint-react", "tslint-react-hooks", "tslint-plugin-prettier", "tslint-config-prettier"],
    "jsRules": {},
    "rules": {
        "prettier": true,
        "jsx-boolean-value": false,
        "react-hooks-nesting": {
            "severity": "error",
            "options": {}
        }
    },
    "rulesDirectory": [],
    "linterOptions": {
        "exclude": ["**/node_modules/**", "**/dist/**", "**/types/**"]
    }
}
